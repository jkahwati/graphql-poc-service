{
  "name": "graphql-poc-service",
  "version": "1.0.0",
  "description": "Graphql POC for Developer SF Community",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc -p tsconfig.prod.json",
    "start": "ts-node -r dotenv/config src/server.ts",
    "start:prod": "node dist/src/server.js",
    "start:dev": "nodemon -r dotenv/config --exec ts-node src/server.ts",
    "test": "jest --collectCoverage --forceExit --detectOpenHandles",
    "jest-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkahwati/graphql-poc-service.git"
  },
  "author": "Jesus Kahwati",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "typescript": "^4.6.3"
  },
  "bugs": {
    "url": "https://github.com/jkahwati/graphql-poc-service/issues"
  },
  "homepage": "https://github.com/jkahwati/graphql-poc-service#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1"
  }
}
